        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.259" />


    <TextView
        android:id="@+id/RESPUESTA1"
        android:layout_width="195dp"
        android:layout_height="82dp"
        android:text="RESPUESTA"
